@model IEnumerable<MovieDAL.Movie>

<!DOCTYPE html>

<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>Index</title>
</head>
<body>
	<h1>Filmer</h1>
	<ul>
		@foreach (var movie in Model)
		{
			<li>
				@Html.ActionLink(movie.Title, "Details", "Movie", new { id = movie.MovieId}, null)
			</li>
		}
	</ul>
</body>
</html>
